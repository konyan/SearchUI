    "lint:fix": "eslint --fix --ext .jsx --ext .tsx --ext .ts --ext .js --ext .test.js ./",
    "type-check": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
